oldProcess=`ps ax | fgrep node | awk '{print $1}' | sed -n 1p`
kill -15 $oldProcess
sleep 1
kill -2 $oldProcess
sleep 1
kill -9 $oldProcess
sleep 1

cd app-root/repo
npm install react-tools browserify envify uglifyjs uglify-js
gem install sass --no-ri --no-rdoc
mkdir _dist
mkdir _dist/static
make server NODE_ENV=production &

while [ ! -f go.txt ]; then
do
  echo "Waiting for go.txt to be created."
  sleep 10
done
